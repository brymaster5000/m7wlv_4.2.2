#!/system/bin/sh

#Set CPU_FREQ
echo 384000 > /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq; 
echo 384000 > /sys/devices/system/cpu/cpu1/cpufreq/scaling_min_freq;
echo 384000 > /sys/devices/system/cpu/cpu2/cpufreq/scaling_min_freq;
echo 384000 > /sys/devices/system/cpu/cpu3/cpufreq/scaling_min_freq;
echo 1728000 > /sys/devices/system/cpu/cpu0/cpufreq/cpuinfo_max_freq; 
echo 1728000 > /sys/devices/system/cpu/cpu1/cpufreq/cpuinfo_max_freq;
echo 1728000 > /sys/devices/system/cpu/cpu2/cpufreq/cpuinfo_max_freq;
echo 1728000 > /sys/devices/system/cpu/cpu3/cpufreq/cpuinfo_max_freq;
